{
  "expo": {
    "name": "Grownet Transport",
    "slug": "Grownettransport",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/iconBlanco.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashBlanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#026CD2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creativejungle.grownettransport",
      "icon": "./assets/iconBlanco.png",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to allow you to take photos of delivery evidence within the app. Your photos will be stored locally on your device, and with your consent, they can be stored in our database as proof that the job was completed."
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconBlancoAndroid.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "STORAGE"
      ],
      "package": "com.creativejungle.grownettransport"
    },
    "extra": {
      "eas": {
        "projectId": "4ebaf71a-7fb1-4617-ad51-51d96de49993"
      }
    }
  }
}
