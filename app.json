{
  "expo": {
    "name": "GrownetTransporters",
    "slug": "GrownetTransporters",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconBlanco.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashBlanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#026CD2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creativejungle.grownetlogistic",
      "icon": "./assets/IconBlanco.png",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconBlancoAndroid.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
